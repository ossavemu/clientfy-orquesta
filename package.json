{
  "name": "clientfy-orquesta",
  "version": "1.0.0",
  "description": "API para gestionar instancias de DigitalOcean",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "start": "bun run dist/app.js",
    "build": "bun build ./src/app.ts --outdir ./dist",
    "dev": "bun run lint:fix && bun --hot run src/app.ts",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix"
  },
  "dependencies": {
    "axios": "1.7.9",
    "express": "^5.0.1",
    "ioredis": "^5.3.2",
    "ksuid": "^3.0.0",
    "nodemailer": "^6.9.12",
    "redis-commander": "^0.8.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.5",
    "@types/nodemailer": "^6.4.4",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "prettier": "^3.4.2",
    "typescript": "^5.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "cpu-features",
      "ssh2",
      "bcrypt",
      "redis-commander"
    ]
  }
}
